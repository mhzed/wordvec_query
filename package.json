    "main": "index.js",
    "scripts": {
        "test": "nodeunit test/",
        "peg": "pegjs --plugin ./node_modules/ts-pegjs/src/tspegjs.js -o src/wordExpression/wordExpression.ts src/wordExpression/wordExpression.pegjs",
        "compile": "tsc",
        "compile-thrift": "thrift-typescript --outDir thrift/ vecquery.thrift",
        "build-docker": "docker build -t mhzed/wordvec_query .",
        "server-nms": "ts-node src/server.ts -p 9999 --nms localhost:10000 --data ../extern/glove.6B",
        "thriftserver": "ts-node src/server.ts -p 9999 -a glove.6B.300d.txt.300.angular.50.annoy --data ../extern/glove.6B/",
        "thriftclient": "ts-node src/client.ts --server localhost:9999",
        "httpserver": "ts-node src/server.ts -p 9999 -a --data ../extern/glove.6B/ -e -x localhost:9300",
        "httpclient": "ts-node src/client.ts --server http://localhost:9999/thrift",
        "index": "node src/index.js -la -t 50",
        "server": "node src/server.js -e -a",
        "client": "node src/client.js"
    },
